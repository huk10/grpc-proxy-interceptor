{
  "name": "@io-huk/grpc-proxy-interceptor",
  "version": "0.0.1-beta.2",
  "type": "module",
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "files": [
    "./lib",
    "./src"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js"
    }
  },
  "scripts": {
    "test": "jest",
    "clean": "rm -rf ./lib",
    "build": "npm run clean && rollup --config rollup.config.js",
    "test:integration": "jest --config ./jest.config.integration.mjs",
    "format": "npx prettier --config ./.perttierrc.yaml --write '**/*.{ts,js,md,mjs}'",
    "startTestServer": "cd ./tests/resources/grpc-server && go run server.go",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/io-huk/grpc-proxy-interceptor.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.6.9",
    "@grpc/proto-loader": "^0.7.0",
    "axios": "^0.25.0",
    "http-status-codes": "^2.2.0",
    "openapi-client-axios": "^5.1.2",
    "qs": "^6.11.0"
  },
  "devDependencies": {
    "openapi-types": "^12.0.0",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-typescript": "^8.3.4",
    "@swc/core": "^1.2.224",
    "@swc/jest": "^0.2.22",
    "@types/jest": "^28.1.6",
    "@types/node": "^17.0.39",
    "@types/qs": "^6.9.7",
    "husky": "^8.0.0",
    "jest": "^28.1.3",
    "pretty-quick": "^3.1.3",
    "rollup": "^2.77.3",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}
